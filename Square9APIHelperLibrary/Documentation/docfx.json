{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
		  "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
	"globalMetadata": {
		"_appTitle": "Square9APIHelperLibrary",
		"_appName": "Square9APIHelperLibrary",
		"_appFaviconPath": "images/Square9icon_16x16.png",
		"_appLogoPath": "images/Square9icon_70x70.png",
		"_appFooter": "<a href='https://github.com/chrisstoll1/Square9APIHelperLibrary'>Square 9 Helper Library</a>",
		"_copyrightFooter": "Â© chrisstoll1. All rights reserved.",
		"_enableSearch": false,
		"_disableSideFilter": false,
		"_enableNewTab": true,
		"_disableContribution": false,
		"_disableBreadcrumb": false
	},
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
	  "templates/singulinkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}